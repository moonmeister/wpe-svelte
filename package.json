{
  "name": "wpe-svelte",
  "description": "Svelte + Atlas + WordPress Example",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "wpe-build": "npm run build",
    "generate": "gqless generate",
    "start": "PORT=8080 node build"
  },
  "devDependencies": {
    "@gqless/cli": "^2.0.16",
    "@sveltejs/kit": "^1.0.0-next.123",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte": "^3.38.3"
  },
  "type": "module",
  "dependencies": {
    "@faustjs/core": "^0.7.0",
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@sveltejs/adapter-node": "^1.0.0-next.32",
    "cookie": "^0.4.1",
    "graphql": "^15.5.1"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
